function MyComponent() {
  useCustomHook();

  while (true) {
    useCustomHook();
    ~~~~~~~~~~~~~~~~ [A hook cannot appear inside a loop]
  }
}

const MyComponentArrowFunction = () => {
  useCustomHook();

  while (true) {
    useCustomHook();
    ~~~~~~~~~~~~~~~~ [A hook cannot appear inside a loop]
  }
}


const MyComponentFunctionExpression = function() {
  useCustomHook();

  while (true) {
    useCustomHook();
    ~~~~~~~~~~~~~~~~ [A hook cannot appear inside a loop]
  }
}
